{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "http://www.mortgagecalculator.org/"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "content-wrapper"
      }
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "name",
        "value": "param[homevalue]"
      },
      "text": "400000"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "td.last"
      }
    },
    {
      "type": "setElementSelected",
      "locator": {
        "type": "xpath",
        "value": "param[credit]/*[. = 'Excellent']"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "input[type=\"submit\"]"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "td > h3"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "content-wrapper"
      }
    },
    {
      "type": "verifyTextPresent",
      "text": "$1,302.42 Monthly Payment"
    }
  ]
}